{
  "name": "elm-web3",
  "version": "0.0.1",
  "description": "An elm wrapper for Web3.js",
  "scripts": {
    "rpc": "testrpc -s 1 -b 4 -i 42",
    "cditch-gethnet": "geth --datadir 'temp/testNode' --networkid 42513 console --ws --wsport 8545 --wsaddr 0.0.0.0 --wsorigins '*'",
    "iditch-gethnet": "geth --datadir 'temp/testNode' --networkid 42512 console --ws --wsport 8545 --wsaddr 0.0.0.0 --wsorigins '*'",
    "web": "python -m SimpleHTTPServer 8000",
    "test": "elm-live test/Main.elm --output=test/elm.js --debug",
    "lightbox-build": "elm-make examples/lightbox/Main.elm --output examples/lightbox/elm.js --yes --debug",
    "testbox-build": "elm-make examples/testbox/Main.elm --output examples/testbox/elm.js --yes --debug",
    "lightbox-live": "elm-live examples/lightbox/Main.elm --output=examples/lightbox/elm.js --open --port=1234 --dir=examples/lightbox",
    "testbox-live": "elm-live examples/testbox/Main.elm --output=examples/testbox/elm.js --open --port=1234 --dir=examples/testbox"
  },
  "author": "Coury Ditch and Nick Miller",
  "license": "to decentrlize!"
}
